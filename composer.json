{
  "name": "joonika/joonika",
  "type": "framework",
  "description": "light open source php framework",
  "keywords": [
    "joonika",
    "joonika framework",
    "framework",
    "library",
    "php",
    "mvc framework",
    "mvc"
  ],
  "homepage": "https://github.com/joonika/joonika",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohsen Jadidi",
      "email": "mnjadidi@gmail.com"
    }
  ],
  "require": {
    "php": ">=7",
    "catfan/medoo": "^2.1.2",
    "whichbrowser/parser": "2.1.1",
    "ezyang/htmlpurifier": "*",
    "fzaninotto/faker": "*",
    "phpfastcache/phpfastcache": "^8.1.0",
    "nesbot/carbon": "^2.42.0",
    "ext-json": "*",
    "symfony/console": "*",
    "curl/curl": "*",
    "symfony/http-foundation": "^5.1",
    "composer/installers": "*",
    "twig/twig": "*",
    "ext-soap": "*",
    "ext-gd": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "ext-curl": "*",
    "phpseclib/phpseclib": "2.0",
    "ext-yaml": "*",
    "zircote/swagger-php": "*",
    "doctrine/annotations": "^1.11",
    "phpoffice/phpspreadsheet": "^1.6",
    "hisune/echarts-php": "1.0.12"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Joonika\\": "src/",
      "Modules\\": "../../../modules/",
      "Themes\\": "../../../themes/",
      "Config\\": "../../../config/"
    },
    "files": [
      "src/Helpers.php"
    ],
    "classmap": [
      "src/dev"
    ]
  }
}